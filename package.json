    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "~50.0.4",
    "prettier": "3.0.3",
    "react-native-launch-arguments": "^4.0.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1",
