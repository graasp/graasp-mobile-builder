    "@types/jest": "^29.5.6",
    "@types/lodash.truncate": "^4",
    "@types/node": "^20.9.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.7.0",
    "detox": "^20.20.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.53.0",
    "eslint-config-universe": "^12.0.0",
