{
  "name": "graasp-mobile-builder",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "engines": {
    "node": "20.0.0"
  },
  "scripts": {
    "start": "expo start --dev-client",
    "start:test": "NODE_ENV=test expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint ./src",
    "build:ts": "tsc --noEmit",
    "build:ios:dev": "env-cmd -f ./.env.development npx eas-cli build -e development-simulator --platform ios --local",
    "build:android:dev": "env-cmd -f ./.env.development npx eas-cli build -e development-simulator --platform android --local",
    "build:ios:prod": "env-cmd -f ./.env.production npx eas-cli build -e production --platform ios --local",
    "build:android:prod": "env-cmd -f ./.env.production npx eas-cli build -e production --platform android --local",
    "build:android:prod-apk": "env-cmd -f ./.env.production npx eas-cli build -e preview --platform android --local",
    "prettier:check": "prettier --check {src,test}/**/*.{js,ts,tsx}",
    "prettier:write": "prettier --write {src,test}/**/*.{js,ts,tsx}",
    "check": "yarn prettier:check && yarn lint && yarn build:ts",
    "eas-build-pre-install": "./eas-hooks/eas-build-pre-install.sh",
    "eas-build-on-success": "./eas-hooks/eas-build-on-success.sh",
    "eas:init": "npx eas-cli init",
    "prebuild:local": "env-cmd -f .env.test npx expo prebuild --platform ios",
    "prebuild:test:ios": "npx expo prebuild --platform ios",
    "prebuild:test:android": "npx expo prebuild --platform android",
    "build:test:ios": "detox build --configuration ios.debug",
    "build:test:android": "detox build --configuration android.debug",
    "test:ios": "env-cmd -f ./.env.test detox test --configuration ios.debug",
    "test:android": "env-cmd -f ./.env.test detox test --configuration android.debug",
    "doctor": "npx expo-doctor"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-arrow-functions": "^7.22.5",
    "@babel/plugin-transform-shorthand-properties": "^7.22.5",
    "@babel/plugin-transform-template-literals": "^7.22.5",
    "@config-plugins/detox": "^6.0.0",
    "@expo/metro-config": "~0.17.1",
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@graasp/query-client": "2.1.0",
    "@graasp/sdk": "3.3.0",
    "@graasp/translations": "1.21.1",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-firebase/analytics": "^18.6.1",
    "@react-native-firebase/app": "^18.6.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@sentry/react-native": "5.20.0",
    "axios": "^1.6.0",
    "expo": "~50.0.17",
    "expo-asset": "~9.0.2",
    "expo-av": "~13.10.5",
    "expo-barcode-scanner": "~12.9.3",
    "expo-build-properties": "~0.11.1",
    "expo-camera": "~14.1.3",
    "expo-constants": "~15.4.6",
    "expo-crypto": "~12.8.1",
